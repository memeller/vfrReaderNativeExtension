<extension xmlns="http://ns.adobe.com/air/extension/14.0">
	  <id>pl.mllr.extensions.vfrPdfReader</id>
	  <versionNumber>2</versionNumber>
	  <platforms>
	      <platform name="iPhone-ARM">
	        <applicationDeployment>
	          <nativeLibrary>libVfrPdfReader.a</nativeLibrary>
	          <initializer>vfrPdfReaderExtInitializer</initializer>
	          <finalizer>vfrPdfReaderExtFinalizer</finalizer>
	        </applicationDeployment>
	      </platform>
          <platform name="iPhone-x86">
              <applicationDeployment>
                  <nativeLibrary>libVfrPdfReader.a</nativeLibrary>
                  <initializer>vfrPdfReaderExtInitializer</initializer>
                  <finalizer>vfrPdfReaderExtFinalizer</finalizer>
              </applicationDeployment>
	      </platform>
          <platform name="default"> 
              <applicationDeployment/> 
          </platform>
	  </platforms>
</extension>
